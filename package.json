{
	"name": "solr",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"check:types": "tsc --pretty --noEmit",
		"check:format": "prettier --check .",
		"lint": "next lint",
		"check:all": "yarn check:format && yarn lint && yarn check:types",
		"format": "prettier --write .",
		"plop": "plop",
		"postinstall": "husky install",
		"generate:graphql-codegen": "graphql-codegen",
		"generate:nexus": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' graphql/schema",
		"generate:types": "yarn generate:graphql-codegen && yarn generate:nexus"
	},
	"dependencies": {
		"@apollo/client": "^3.8.3",
