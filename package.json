		"@types/react-dom": "18.2.7",
		"@types/react-slider": "^1.3.1",
		"@types/three": "^0.156.0",
		"@typescript-eslint/eslint-plugin": "^6.6.0",
		"@typescript-eslint/parser": "^6.6.0",
		"eslint": "8.48.0",
		"eslint-config-next": "13.4.19",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
