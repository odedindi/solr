		"@mantine/carousel": "^5.2.3",
		"@mantine/core": "^5.2.3",
		"@mantine/hooks": "^5.2.3",
		"@mantine/modals": "^5.2.3",
		"@mantine/next": "^5.2.3",
		"@mantine/notifications": "^5.2.3",
		"@mantine/nprogress": "^5.2.3",
		"@mantine/spotlight": "^5.2.3",
		"@react-spring/three": "^9.5.2",
		"@react-three/drei": "^9.23.8",
		"@react-three/fiber": "^8.6.0",
		"@react-three/postprocessing": "^2.6.2",
		"@tabler/icons": "^1.96.0",
		"apollo-server-core": "^3.10.2",
		"apollo-server-micro": "^3.10.2",
		"embla-carousel-react": "^7.0.1",
		"graphql": "^16.6.0",
		"graphql-tag": "^2.12.6",
		"gsap": "^3.11.0",
		"lodash": "^4.17.21",
		"micro": "^9.4.1",
		"next": "12.2.5",
		"next-pwa": "^5.6.0",
		"nexus": "^1.3.0",
		"postprocessing": "^6.28.7",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-slider": "^2.0.4",
		"react-spring": "^9.5.2",
		"rxjs": "^7.5.6",
		"three": "^0.143.0"
	},
