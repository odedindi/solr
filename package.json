		"graphql": "^16.8.0",
		"graphql-tag": "^2.12.6",
		"gsap": "^3.12.2",
		"lodash": "^4.17.21",
		"micro": "^10.0.1",
		"next": "13.4.19",
		"next-pwa": "^5.6.0",
		"nexus": "^1.3.0",
		"postprocessing": "^6.33.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-slider": "^2.0.6",
		"react-spring": "^9.7.2",
		"tabler-icons-react": "^1.56.0",
		"three": "^0.156.1"
	},
